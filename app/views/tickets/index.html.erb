<h1>Listing tickets</h1>

<table>
  <thead>
    <tr>
      <th>Folio</th>
      <th>Nombre</th>
      <th>Correo</th>
      <th>Vendido</th>
      <th>Trayecto</th>
      <th>Precio</th>
      <th>Puntos</th>
      <th>Viaje</th>
      <th>Vendido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.folio %></td>
        <td><%= ticket.nombre %></td>
        <td><%= ticket.correo %></td>
        <td><%= ticket.vendido %></td>
        <td><%= ticket.trayecto %></td>
        <td><%= ticket.precio %></td>
        <td><%= ticket.puntos %></td>
        <td><%= ticket.viaje %></td>
        <td><%= ticket.vendido %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
