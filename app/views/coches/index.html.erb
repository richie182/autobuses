<h1>Listing coches</h1>

<table>
  <thead>
    <tr>
      <th>Matricula</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coches.each do |coch| %>
      <tr>
        <td><%= coch.matricula %></td>
        <td><%= coch.user_id %></td>
        <td><%= link_to 'Show', coch %></td>
        <td><%= link_to 'Edit', edit_coch_path(coch) %></td>
        <td><%= link_to 'Destroy', coch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<br><%= link_to 'Menu principar',  :controller => "principal", :action => "index"%></br>
